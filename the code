def binary_search(arr, x):
    low = 0
    high = len(arr) - 1
    while low <= high:
        mid = (low + high) // 2
        if arr[mid] == x:
            return mid
        elif arr[mid] < x:
            low = mid + 1
        else:
            high = mid - 1
    return -1

arr = [ 69, 420, 57, 12, 639, 71, 100, 47, 99,10 ]
x = 10
if binary_search(arr,x) :
  print ("Found at")
else: print ("Not Found")
  
